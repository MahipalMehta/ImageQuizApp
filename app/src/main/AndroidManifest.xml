<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.Mahipal.bharatiWordQuiz">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/mylogo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name="com.Mahipal.bharatiWordQuiz.QuixStartActivity2"></activity>
        <activity android:name="com.Mahipal.bharatiWordQuiz.Quiz2" />
        <activity
            android:name="com.Mahipal.bharatiWordQuiz.UI"
            android:label="@string/title_activity_u_i"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:name="com.Mahipal.bharatiWordQuiz.ExitPage"
            android:screenOrientation="portrait"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:name="com.Mahipal.bharatiWordQuiz.QuizStartActicity"
            android:screenOrientation="portrait"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:name="com.Mahipal.bharatiWordQuiz.Quiz"
            android:screenOrientation="portrait"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:name="com.Mahipal.bharatiWordQuiz.Splash"
            android:screenOrientation="portrait"
            tools:ignore="LockedOrientationActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name="com.Mahipal.bharatiWordQuiz.music.MusicService"
            android:enabled="true" />
    </application>

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>